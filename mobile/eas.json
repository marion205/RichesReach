{
"cli": {
"version": ">= 5.9.0"
},
"build": {
"development": {
"developmentClient": true,
"distribution": "internal"
},
"preview": {
"distribution": "internal",
"ios": {
"simulator": true
}
},
"production": {
"ios": {
"autoIncrement": "buildNumber"
},
"android": {
"buildType": "app-bundle"
},
"env": {
"EXPO_PUBLIC_API_URL": "https://app.richesreach.net",
"EXPO_PUBLIC_GRAPHQL_URL": "https://app.richesreach.net/graphql",
"EXPO_PUBLIC_RUST_API_URL": "https://app.richesreach.net:3001",
"EXPO_PUBLIC_ENVIRONMENT": "production"
}
},
"internal": {
"android": {
"buildType": "apk"
},
"env": {
"EXPO_PUBLIC_API_URL": "https://app.richesreach.net",
"EXPO_PUBLIC_GRAPHQL_URL": "https://app.richesreach.net/graphql",
"EXPO_PUBLIC_RUST_API_URL": "https://app.richesreach.net:3001",
"EXPO_PUBLIC_ENVIRONMENT": "production"
}
}
},
"submit": {
"production": {
"ios": {
"appleId": "marion@richesreach.com",
"ascAppId": "com.richesreach.app",
"appleTeamId": "YOUR_APPLE_TEAM_ID"
},
"android": {
"serviceAccountKeyPath": "./google-service-account.json",
"track": "production"
}
}
}
}