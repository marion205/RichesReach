{
  "name": "richesreach",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client",
    "start:go": "expo start --go",
    "build:dev:ios": "eas build --profile simulator --platform ios",
    "build:dev:ios:local": "eas build --profile simulator --platform ios --local",
    "build:dev:android": "eas build --profile development --platform android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "performance": "expo start --no-dev --minify",
    "analyze": "expo start --analyze",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:phase1": "jest --testPathPattern=test_phase1_components.test.tsx",
    "test:phase2": "jest --testPathPattern=test_phase2_components.test.tsx",
    "test:phase3": "jest --testPathPattern=test_phase3_components.test.tsx",
    "test:e2e": "detox test",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:record": "detox test --record-videos all",
    "build:e2e:ios": "detox build --configuration ios.sim.debug",
    "build:e2e:android": "detox build --configuration android.emu.debug",
    "demo:record": "node scripts/demo-recorder.js",
    "bundle:android": "bash scripts/measure-bundle-size.sh android",
    "bundle:ios": "bash scripts/measure-bundle-size.sh ios",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "bundle:analyze:ios": "node scripts/analyze-bundle.js ios",
    "bundle:analyze:android": "node scripts/analyze-bundle.js android",
    "optimize:images": "bash scripts/optimize-images.sh",
    "benchmark": "bash scripts/benchmark.sh",
    "benchmark:expo": "bash scripts/benchmark-expo.sh"
  },
