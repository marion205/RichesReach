                },
                {
                    "name": "DEBUG",
                    "value": "False"
                },
                {
                    "name": "ALLOWED_HOSTS",
                    "value": "app.richesreach.com,localhost,127.0.0.1,98.81.210.223"
                },
                {
                    "name": "CSRF_TRUSTED_ORIGINS",
                    "value": "https://app.richesreach.com,http://98.81.210.223:8000"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
