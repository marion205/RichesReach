[
  {
    "name": "riches-reach-ai",
    "image": "498606688292.dkr.ecr.us-east-1.amazonaws.com/riches-reach-ai:65c68050ea03e1249927b8ab5fcd89084c05f72e",
    "cpu": 0,
    "portMappings": [
      {
        "containerPort": 8000,
        "hostPort": 8000,
        "protocol": "tcp"
      }
    ],
    "essential": true,
    "entryPoint": [
      "/bin/sh",
      "-c"
    ],
    "command": [
      "cd /app && python manage.py migrate --noinput && python manage.py collectstatic --noinput || true && exec gunicorn richesreach.wsgi:application --bind 0.0.0.0:8000 --workers 3 --timeout 120"
    ],
    "environment": [
      {
        "name": "DJANGO_SETTINGS_MODULE",
        "value": "richesreach.settings"
      },
      {
        "name": "DJANGO_DB_ENGINE",
        "value": "django.db.backends.postgresql"
      },
      {
        "name": "GRAPHQL_MODE",
        "value": "full"
      },
      {
        "name": "DEBUG",
        "value": "False"
      },
      {
        "name": "STATIC_ROOT",
        "value": "/app/staticfiles"
      },
      {
        "name": "ALLOWED_HOSTS",
        "value": "*"
      }
    ],
    "mountPoints": [],
    "volumesFrom": [],
    "secrets": [
      {
        "name": "DJANGO_DB_HOST",
        "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/prod/db:PGHOST::"
      },
      {
        "name": "DJANGO_DB_PORT",
        "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/prod/db:PGPORT::"
      },
      {
        "name": "DJANGO_DB_USER",
        "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/prod/db:PGUSER::"
      },
      {
        "name": "DJANGO_DB_PASSWORD",
        "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/prod/db:PGPASSWORD::"
      },
      {
        "name": "DJANGO_DB_NAME",
        "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/prod/db:PGDATABASE::"
      },
      {
        "name": "SSLMODE",
        "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/prod/db:SSLMODE::"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/riches-reach-ai",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "healthCheck": {
      "command": [
        "CMD-SHELL",
        "curl -f http://localhost:8000/health/ || exit 1"
      ],
      "interval": 30,
      "timeout": 5,
      "retries": 3,
      "startPeriod": 120
    },
    "systemControls": []
  }
]
