{
  "family": "test-new-appuser-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::498606688292:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::498606688292:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "test-new-appuser",
      "image": "postgres:15",
      "essential": true,
      "command": [
        "psql",
        "-h", "riches-reach-postgres.cmhsue8oy30k.us-east-1.rds.amazonaws.com",
        "-p", "5432",
        "-U", "appuser",
        "-d", "appdb",
        "-c", "SELECT 'Connection successful!' as status;",
        "-c", "SELECT current_database(), current_user;"
      ],
      "secrets": [
        {
          "name": "PGPASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/db/appuser-new-nxOSRF:PASSWORD::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/test-new-appuser",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
