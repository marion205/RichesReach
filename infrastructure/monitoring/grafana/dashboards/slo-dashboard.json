{
  "dashboard": {
    "title": "RichesReach SLO Dashboard",
    "tags": ["slo", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Latency p50/p95",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Latency (ms)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "ML Inference Latency p95",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ml_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Latency (ms)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(api_requests_total{status=~\"2..\"}[5m])) / sum(rate(api_requests_total[5m]))",
            "legendFormat": "Success Rate"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Success Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(redis_cache_hits_total[5m])) / sum(rate(redis_cache_requests_total[5m]))",
            "legendFormat": "Cache Hit Rate"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Hit Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "SLO Compliance Status",
        "type": "stat",
        "targets": [
          {
            "expr": "slo_compliant",
            "legendFormat": "Compliant"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "textMode": "auto"
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "LLM Cost per MAU",
        "type": "stat",
        "targets": [
          {
            "expr": "llm_cost_per_mau",
            "legendFormat": "Cost"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "textMode": "auto"
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16}
      },
      {
        "id": 7,
        "title": "Cost per Decision",
        "type": "stat",
        "targets": [
          {
            "expr": "cost_per_decision",
            "legendFormat": "Cost"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "textMode": "auto"
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(api_requests_total{status=~\"5..\"}[5m]))",
            "legendFormat": "5xx Errors"
          },
          {
            "expr": "sum(rate(api_requests_total{status=~\"4..\"}[5m]))",
            "legendFormat": "4xx Errors"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      }
    ],
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1
  }
}

