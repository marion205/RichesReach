{
"family": "riches-reach-ai-task",
"networkMode": "awsvpc",
"requiresCompatibilities": [
"FARGATE"
],
"cpu": "512",
"memory": "1024",
"executionRoleArn": "arn:aws:iam::498606688292:role/ecsTaskExecutionRole",
"taskRoleArn": "arn:aws:iam::498606688292:role/ecsTaskRole",
"containerDefinitions": [
{
"name": "riches-reach-ai",
"image": "498606688292.dkr.ecr.us-east-1.amazonaws.com/riches-reach-ai:latest",
"portMappings": [
{
"containerPort": 8000,
"protocol": "tcp"
}
],
"essential": true,
"workingDirectory": "/app",
"environment": [
{
"name": "ENVIRONMENT",
"value": "production"
},
{
"name": "AWS_REGION",
"value": "us-east-1"
}
],
"secrets": [
{
"name": "DATABASE_URL",
"valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:richesreach/DATABASE_URL-3FiSbV"
}
],
"logConfiguration": {
"logDriver": "awslogs",
"options": {
"awslogs-group": "/ecs/riches-reach-ai",
"awslogs-region": "us-east-1",
"awslogs-stream-prefix": "ecs"
}
}
}
]
}