{
  "family": "riches-reach-ai-task",
  "taskRoleArn": "arn:aws:iam::498606688292:role/ecsTaskRole",
  "executionRoleArn": "arn:aws:iam::498606688292:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "riches-reach-ai",
      "image": "498606688292.dkr.ecr.us-east-1.amazonaws.com/riches-reach-ai:PLACEHOLDER_IMAGE_TAG",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "command": [
        "bash",
        "-lc",
        "echo '=== PRODUCTION DEPLOYMENT ===' && echo 'Environment check:' && echo 'DJANGO_DB_ENGINE:' $DJANGO_DB_ENGINE && echo 'DJANGO_DB_HOST:' $DJANGO_DB_HOST && echo 'DJANGO_DB_NAME:' $DJANGO_DB_NAME && echo 'DJANGO_DB_USER:' $DJANGO_DB_USER && echo 'DJANGO_DB_PASSWORD:' 'SET' && echo 'DJANGO_DB_PORT:' $DJANGO_DB_PORT && echo 'Running migrations...' && python manage.py migrate --noinput && echo 'Collecting static files...' && python manage.py collectstatic --noinput && echo 'Starting Gunicorn server...' && exec gunicorn richesreach.wsgi:application --bind 0.0.0.0:8000 --workers 3 --timeout 120 --access-logfile - --error-logfile -"
      ],
      "environment": [
        {
          "name": "DJANGO_SETTINGS_MODULE",
          "value": "richesreach.settings"
        },
        {
          "name": "DEBUG",
          "value": "False"
        },
        {
          "name": "STATIC_ROOT",
          "value": "/app/staticfiles"
        },
        {
          "name": "ALLOWED_HOSTS",
          "value": "*"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "DJANGO_DB_ENGINE",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/db/appuser-PE1IS5:DJANGO_DB_ENGINE::"
        },
        {
          "name": "DJANGO_DB_NAME",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/db/appuser-PE1IS5:DJANGO_DB_NAME::"
        },
        {
          "name": "DJANGO_DB_USER",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/db/appuser-PE1IS5:DJANGO_DB_USER::"
        },
        {
          "name": "DJANGO_DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/db/appuser-PE1IS5:DJANGO_DB_PASSWORD::"
        },
        {
          "name": "DJANGO_DB_HOST",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/db/appuser-PE1IS5:DJANGO_DB_HOST::"
        },
        {
          "name": "DJANGO_DB_PORT",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr/db/appuser-PE1IS5:DJANGO_DB_PORT::"
        },
        {
          "name": "SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:498606688292:secret:rr-django-secret-key-IdsacH"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/riches-reach-ai",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -fsS http://localhost:8000/health/ || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      },
      "systemControls": []
    }
  ],
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  }
}
