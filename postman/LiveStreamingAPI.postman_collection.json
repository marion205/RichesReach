      "name": "Live Streams",
      "item": [
        {
          "name": "List Live Streams",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{base_url}}/live-streams/?circle_id={{circle_id}}&status=live&page=1&limit=20",
              "host": ["{{base_url}}"],
              "path": ["live-streams", ""],
              "query": [
                {
                  "key": "circle_id",
                  "value": "{{circle_id}}"
                },
                {
                  "key": "status",
                  "value": "live"
                },
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "20"
                }
              ]
            }
          },
          "response": []
