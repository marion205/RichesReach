{
    "family": "create-appdb-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::498606688292:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::498606688292:role/ecsTaskRole",
    "containerDefinitions": [
        {
            "name": "create-appdb",
            "image": "postgres:15",
            "essential": true,
            "command": [
                "psql",
                "-h", "riches-reach-postgres.cmhsue8oy30k.us-east-1.rds.amazonaws.com",
                "-p", "5432",
                "-U", "richesreach",
                "-d", "postgres",
                "-c", "CREATE DATABASE appdb;"
            ],
            "environment": [
                {
                    "name": "PGPASSWORD",
                    "value": "TEMP_Master_2024!ChangeMe"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/create-appdb",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
